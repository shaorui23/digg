- if @cmd == :info
  - @result.each do |item|
    %span= item.join ':'
    %br
- elsif @result.respond_to? :each_with_index
  - @result.each_with_index do |item, index|
    %span= "#{index+1}) \"#{item}\""
    %br
- elsif @result.is_a? Fixnum
  %span= "(integer) #{@result}"
  %br
- else
  - @result = "\"#{@result}\"" unless unquoted.include? @result
  %span= @result
  %br
