<h1>Set</h1>
<fieldset>
  <dl>
    <dt class="key_field label string_type"><label>Key<dd><pre class="prettyprint"><%= @redisset.redis_key %></dd></pre></label></dt>
    <dt class="key_field label string_type"><label>size<dd><pre class="prettyprint"><%= Redis.current.scard @redisset.redis_key %></dd></pre></label></dt>
    <dt class="value_field label string_type"><label>Value<dd><pre class="prettyprint"> <% @redisset.redis_value.each do |v| %> <%= v %> <% end %> </dd></pre></label></dt>
    <dt class="value_field label string_type"><label>Type<dd><pre class="prettyprint"><%= @redisset.redis_type %></dd></pre></label></dt>
    <dt class="value_field label string_type"><label>TTL<dd><pre class="prettyprint"><%= @redisset.ttl %></dd></pre></label></dt>
  </dl>
</fieldset>

<div class="form-actions">
  <%= link_to 'Back', redishashes_path, :class => 'btn'  %>
  <%= link_to 'Edit', edit_redishash_path(@redisset), :class => 'btn' %>
  <%= link_to 'Delete', redishash_path(@redisset), :method => 'delete', :confirm => 'Are you sure?', :class => 'btn btn-danger' %>
</div>
