<h1>Redis Sorted Set</h1>

<fieldset>
  <dl>
    <dt class="key_field label string_type"><label>Key<dd><pre class="prettyprint"><%= @zset.redis_key %></dd></pre></label></dt>
    <dt class="value_field label string_type"><label>Value<dd><pre class="prettyprint"><%= @zset.redis_value %></dd></pre></label></dt>
    <dt class="value_field label string_type"><label>Type<dd><pre class="prettyprint"><%= @zset.redis_type %></dd></pre></label></dt>
    <dt class="value_field label string_type"><label>TTL<dd><pre class="prettyprint"><%= @zset.ttl %></dd></pre></label></dt>
  </dl>
</fieldset>

<div class="form-actions">
  <%= link_to 'Back', objecteds_path, :class => 'btn'  %>
  <%= link_to 'Edit', edit_objected_path(@zset), :class => 'btn' %>
  <%= link_to 'Delete', objected_path(@zset), :method => 'delete', :confirm => 'Are you sure?', :class => 'btn btn-danger' %>
</div>
